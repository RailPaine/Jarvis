<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.jarvis.zhoufeng.jarvis">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/jarvis_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".account.activity.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".account.activity.account.AccountActivity" />

        <service android:name=".account.dev.FloatService" />
    </application>

</manifest>
